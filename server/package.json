{
  "name": "server",
  "version": "1.1.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "biome format --write ./src",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "prebuild": "prisma generate && prisma migrate deploy",
    "postbuild": "npx prisma db seed",
    "build": "tsup src --format esm",
    "start": "node dist/infra/http/server.mjs --env-file .env",
    "dev": "tsx watch --env-file .env.local src/infra/http/server.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --no-file-parallelism --inspect --logHeapUsage ",
    "test:e2e": "vitest run --config ./vitest.config.e2e.mts",
    "test:e2e:watch": "vitest --config ./vitest.config.e2e.mts"
  },
  "prisma": {
    "seed": "node ./prisma/seed.mjs"
  },
  "keywords": ["varuna", "biome", "fastify", "prisma", "vitest", "typescript", "aws", "IT"],
  "author": "Pedro Mendes",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@faker-js/faker": "^8.4.1",
    "@types/node": "^20.16.6",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^2.1.1",
    "fastify-type-provider-zod": "^2.1.0",
    "prisma": "^5.20.0",
    "supertest": "^7.0.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "unplugin-swc": "^1.5.1",
    "vite": "^5.4.7",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.665.0",
    "@aws-sdk/s3-request-presigner": "^3.665.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/multipart": "^8.3.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^3.1.0",
    "@prisma/client": "5.20.0",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.5.1",
    "vitest": "^2.1.1",
    "zod": "^3.23.8"
  },
  "packageManager": "pnpm@9.1.0+sha512.67f5879916a9293e5cf059c23853d571beaf4f753c707f40cb22bed5fb1578c6aad3b6c4107ccb3ba0b35be003eb621a16471ac836c87beb53f9d54bb4612724"
}
